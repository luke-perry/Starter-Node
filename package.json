{
    "name": "Starter-Node",
    "version": "1.0.0",
    "description": "Quick node starter app with basic needs",
    "main": "App.js",
    "scripts": {
      "start": "node App.js",
      "dev": "nodemon",
      "test": "jest --watch --testPathIgnorePatterns ./acceptance",
      "test-cov": "jest --coverage --testPathIgnorePatterns ./acceptance",
      "acceptance": "jest ./acceptance",
      "lint": "node_modules/eslint/bin/eslint.js .",
      "pre-commit": "npm run lint && npm run test-cov && npm run acceptance"
    },
    "author": "Luke Perry",
    "license": "ISC",
    "engines": {
      "node": ">=10.0.0"
    },
    "dependencies": {
      "express": "^4.17.1"
    },
    "devDependencies": {
      "eslint": "^6.8.0",
      "eslint-config-airbnb-base": "^14.0.0",
      "eslint-plugin-import": "^2.20.1",
      "eslint-plugin-jest": "^23.7.0",
      "jest": "^25.1.0",
      "jest-extended": "0.11.5",
      "nock": "^12.0.0"
    }
  }
  